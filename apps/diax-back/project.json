{
  "name": "diax-back",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/diax-back/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "options": {
        "sourceMap": true,
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/diax-back",
        "main": "apps/diax-back/src/main.ts",
        "tsConfig": "apps/diax-back/tsconfig.app.json",
        "assets": [ "apps/diax-back/src/assets" ],
        "webpackConfig": "apps/diax-back/webpack.config.js"
      },
      "configurations": {
        "production": {
          "sourceMap": false,
          "optimization": true,
          "extractLicenses": true,
          "outputPath": "dist/apps/diax-back",
          "fileReplacements": [
            {
              "replace": "apps/diax-back/src/environments/environment.ts",
              "with": "apps/diax-back/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "diax-back:build",
        "watch": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["apps/diax-back/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/diax-back/jest.config.ts",
        "logHeapUsage": true
      },
      "configurations": {
        "ci": {
          "maxWorkers": 2
        }
      }
    }
  }
}